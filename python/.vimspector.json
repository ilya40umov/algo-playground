{
  "configurations": {
    "Launch": {
      "adapter": "debugpy",
      "filetypes": [
        "python"
      ],
      "configuration": {
        "name": "Launch",
        "type": "python",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "program": "${file}",
        "python": "/usr/bin/python3",
        "stopOnEntry": true,
        "console": "externalTerminal",
        "debugOptions": [],
        "env": {
          "PYTHONPATH": "${workspaceRoot}"
        }
      }
    }
  }
}
